{
  "name": "bip69",
  "version": "3.0.0",
  "description": "Lexicographical Indexing of Bitcoin Transaction Inputs and Outputs.",
  "author": "Daniel Cousens",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoinjs/bip69.git"
  },
  "bugs": {
    "url": "https://github.com/bitcoinjs/bip69/issues"
  },
  "homepage": "https://github.com/bitcoinjs/bip69",
  "keywords": [
    "bip69",
    "bitcoin"
  ],
  "main": "./bitcoinjs.js",
  "scripts": {
    "coverage": "mocha --require blanket -R travis-cov",
    "coverage-local": "mocha --require blanket -R html-cov",
    "standard": "standard",
    "test": "npm run standard && npm run unit",
    "unit": "mocha"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 100
    }
  },
  "dependencies": {
    "buffer-compare": "^1.0.0"
  },
  "devDependencies": {
    "blanket": "*",
    "mocha": "*",
    "standard": "*",
    "travis-cov": "*"
  }
}
